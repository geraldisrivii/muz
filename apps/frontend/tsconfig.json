{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "#/*": [
        "./src/*"
      ],
      "@/app/*": [
        "./app/*"
      ],
      "@internal/*": [
        "../../internal/*"
      ]
    },
    "strictNullChecks": true,
    "resolveJsonModule": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/signup/page.tsx",
    "middleware.ts",
    "app/page.tsx",
    "server.ts",
    "app/lib/data.js",
    "src/features/pagination/ui/pagination.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
